{
    "FastGithub": {
        "HttpProxyPort": 38457,
        "FallbackDns": [
            "8.8.8.8:53",
            "114.114.114.114:53"
        ],
        "DomainConfigs": {
            "*": {
                "TlsSni": true,
                "TlsSniPattern": null,
                "TlsIgnoreNameMismatch": true,
                "Timeout": null,
                "IPAddress": null,
                "Destination": null,
                "Response": {
                    "StatusCode": 404,
                    "ContentType": "text/plain;charset=utf-8",
                    "ContentValue": "这是一个用于示范配置的域名"
                }
            }
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Yarp": "Warning",
                "System": "Warning",
                "Microsoft": "Warning",
                "Microsoft.AspNetCore.Server.Kestrel": "Error"
            }
        }
    }
}
